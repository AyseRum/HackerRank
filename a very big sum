#include <stdio.h>

int main(void)
{
    long int sum = 0;
    int n; 
    scanf("%d",&n);
    long int arr[n];
    for(long int i = 0; i < n; i++)
        {
         scanf("%ld",&arr[i]);
        }//for
    for(long int i = 0; i < n; i++)
        {
        sum += arr[i];
        }
    printf("%ld", sum);
    return 0;
}
